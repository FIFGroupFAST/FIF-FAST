	<queryString>
		<![CDATA[SELECT
  ppt.company_id,
  ppt.person_id,
  bc.company_name,
  BH.BRANCH_NAME,
  WO.ORGANIZATION_NAME,
  WL.LOCATION_NAME,
  WJ.JOB_NAME,
  (SELECT wo.organization_name
  FROM hierarchy_structural hs
  JOIN wos_organizations wo
  ON wo.organization_id = hs.organization_id
  JOIN wos_organization_versions wov
  ON wov.organization_id = hs.branch_id
  AND TRUNC(sysdate) BETWEEN wov.date_from AND wov.date_to
  WHERE wo.company_id    = ppi.company_id
  AND wov.level_code     = 'DEPT'
  AND wo.organization_id = ppa.organization_id
  AND rownum             = 1
  ) DEPARTMENT_NAME,
  PPI.EMPLOYEE_NUMBER,
  PPI.FULL_NAME,
  PPT.PEOPLE_TYPE,
  PPT.EMPLOYMENT_CATEGORY,
  WJV.IS_KEY_JOB key_people,
  homebase.location_name homebase_name,
  ppa.transfered_by,
  ppa.housing_allowance,
  ppi.birth_date,
  ppi.birth_place,
  ppi.gender_code,
  ppi.religion_code,
  ppi.blood_type,
  edu.education_level,
  edu.institution,
  edu.faculty,
  edu.major,
  edu.faculty || ' / ' || edu.major education_major,
  address.address_line,
  address.provinsi,
  address.city,
  address.zip_code,
  PPI.MARITAL_STATUS,
  ppi.ptkp_status,
  tmc.name_full bank_name,
  pbi.branch_name bank_branch_name,
  pbi.ACCOUNT_NUMBER,
  pbi.ACCOUNT_NAME,
  ppi.hire_date,
  ppi.astra_join_date,
  tr.termination_date,
  tt.termination_reason,
  pai.kpj_number,
  pai.dpa_number,
  pai.npwp,
  NVL(
  (SELECT effective_start_date
  FROM pea_primary_assignments pa1
  WHERE effective_start_date =
    (SELECT MAX(effective_start_date)
    FROM pea_primary_assignments pa2
    WHERE ACTION_TYPE         = 'PROMOTION'
    AND effective_start_date <= TRUNC(sysdate)
    AND pa2.person_id         = pa1.person_id
    AND pa2.company_id        = pa1.company_id
    )
  AND pa1.person_id  = ppi.person_id
  AND pa1.company_id = ppi.company_id
  ), ppi.hire_date) last_grade_date,
  (SELECT MAX(effective_start_date)
  FROM pay_salaries
  WHERE person_id           = ppi.person_id
  AND company_id            = ppi.company_id
  AND effective_start_date <= TRUNC(sysdate)
  ) last_salary_increase_date,
  ppa.effective_start_date last_job_date,
  ppa.effective_start_date last_location_date,
  pai.national_id_number,
  ppa.action_type,
  (select ppi2.employee_number || '-' ||ppi2.full_name from hierarchy_structural hs
join wos_organizations wo on hs.branch_id = wo.organization_id
join wos_organization_versions wov on wov.organization_id = wo.organization_id and trunc(sysdate) between wov.date_from and wov.date_to
join pea_primary_assignments ppa2 on ppa2.organization_id = wo.organization_id and ppa2.job_id = wov.head_of_organization and trunc(sysdate) between ppa2.effective_start_date and ppa2.effective_end_date
join pea_personal_informations ppi2 on ppi2.person_id = ppa2.person_id and ppi2.company_id = ppa2.company_id and trunc(sysdate) between ppi2.effective_start_date and ppi2.effective_end_date
join pea_people_types ppt on ppt.person_id = ppa2.person_id and ppt.company_id = ppa2.company_id and trunc(sysdate) between ppt.effective_start_date and ppt.effective_end_date and ppt.people_type = 'EMPLOYEE'
where hs.organization_id = ppa.organization_id
and ppi2.person_id <> ppi.person_id
and hs.level_org = (select min(hs.level_org)from hierarchy_structural hs
join wos_organizations wo on hs.branch_id = wo.organization_id
join wos_organization_versions wov on wov.organization_id = wo.organization_id and trunc(sysdate) between wov.date_from and wov.date_to
join pea_primary_assignments ppa2 on ppa2.organization_id = wo.organization_id and ppa2.job_id = wov.head_of_organization and trunc(sysdate) between ppa2.effective_start_date and ppa2.effective_end_date
join pea_personal_informations ppi2 on ppi2.person_id = ppa2.person_id and ppi2.company_id = ppa2.company_id and trunc(sysdate) between ppi2.effective_start_date and ppi2.effective_end_date
join pea_people_types ppt on ppt.person_id = ppa2.person_id and ppt.company_id = ppa2.company_id and trunc(sysdate) between ppt.effective_start_date and ppt.effective_end_date and ppt.people_type = 'EMPLOYEE'
where hs.organization_id = ppa.organization_id
and ppi2.person_id <> ppi.person_id)
and rownum <= 1
) supervisor,
(select ppi2.employee_number || '-' ||ppi2.full_name from hierarchy_structural hs
join wos_organizations wo on hs.branch_id = wo.organization_id
join wos_organization_versions wov on wov.organization_id = wo.organization_id and trunc(sysdate) between wov.date_from and wov.date_to
join wos_job_versions wjv on wjv.job_id = wov.head_of_organization and trunc(sysdate) between wjv.date_from and wjv.date_to and wjv.is_manager = 1
join pea_primary_assignments ppa2 on ppa2.organization_id = wo.organization_id and ppa2.job_id = wov.head_of_organization and trunc(sysdate) between ppa2.effective_start_date and ppa2.effective_end_date
join pea_personal_informations ppi2 on ppi2.person_id = ppa2.person_id and ppi2.company_id = ppa2.company_id and trunc(sysdate) between ppi2.effective_start_date and ppi2.effective_end_date
join pea_people_types ppt on ppt.person_id = ppa2.person_id and ppt.company_id = ppa2.company_id and trunc(sysdate) between ppt.effective_start_date and ppt.effective_end_date and ppt.people_type = 'EMPLOYEE'
where hs.organization_id = ppa.organization_id
and ppi2.person_id <> ppi.person_id
and hs.level_org = (select min(hs.level_org) from hierarchy_structural hs
join wos_organizations wo on hs.branch_id = wo.organization_id
join wos_organization_versions wov on wov.organization_id = wo.organization_id and trunc(sysdate) between wov.date_from and wov.date_to
join wos_job_versions wjv on wjv.job_id = wov.head_of_organization and trunc(sysdate) between wjv.date_from and wjv.date_to and wjv.is_manager = 1
join pea_primary_assignments ppa2 on ppa2.organization_id = wo.organization_id and ppa2.job_id = wov.head_of_organization and trunc(sysdate) between ppa2.effective_start_date and ppa2.effective_end_date
join pea_personal_informations ppi2 on ppi2.person_id = ppa2.person_id and ppi2.company_id = ppa2.company_id and trunc(sysdate) between ppi2.effective_start_date and ppi2.effective_end_date
join pea_people_types ppt on ppt.person_id = ppa2.person_id and ppt.company_id = ppa2.company_id and trunc(sysdate) between ppt.effective_start_date and ppt.effective_end_date and ppt.people_type = 'EMPLOYEE'
where hs.organization_id = ppa.organization_id
and ppi2.person_id <> ppi.person_id)
and rownum <= 1) manager
FROM PEA_PERSONAL_INFORMATIONS PPI
JOIN PEA_PRIMARY_ASSIGNMENTS PPA
ON PPA.PERSON_ID   = PPI.PERSON_ID
AND PPA.COMPANY_ID = PPI.COMPANY_ID
JOIN WOS_ORGANIZATIONS WO
ON WO.ORGANIZATION_ID = PPA.ORGANIZATION_ID
JOIN WOS_JOBS WJ
ON WJ.JOB_ID = PPA.JOB_ID
JOIN WOS_GRADES WG
ON WG.GRADE_ID = PPA.GRADE_ID
LEFT JOIN WOS_ORGANIZATION_VERSIONS WOV
ON WOV.ORGANIZATION_ID = PPA.ORGANIZATION_ID
AND TRUNC(TO_DATE($P{PROCESS_DATE} ,'DD-MON-YYYY')) BETWEEN wov.date_from AND wov.date_to
LEFT JOIN WOS_LOCATIONS wl
ON WL.LOCATION_ID = wov.location_id
AND wl.company_id = ppi.company_id
JOIN BSE_COMPANIES bc
ON bc.company_id = ppi.company_id
LEFT JOIN WOS_JOB_VERSIONS WJV
ON WJV.JOB_ID = PPA.JOB_ID
AND TRUNC(TO_DATE($P{PROCESS_DATE} ,'DD-MON-YYYY')) BETWEEN wjv.date_from AND wjv.date_to
JOIN PEA_PEOPLE_TYPES ppt
ON ppt.person_id   = ppi.person_id
AND ppt.company_id = ppi.company_id
JOIN WOS_LOCATIONS homebase
ON homebase.location_code = ppi.home_base_code
AND homebase.company_id   = ppi.company_id
LEFT JOIN PEA_BANK_INFORMATIONS pbi
ON pbi.person_id   = ppi.person_id
AND pbi.company_id = ppi.company_id
AND TRUNC(TO_DATE($P{PROCESS_DATE} ,'DD-MON-YYYY')) BETWEEN pbi.effective_start_date AND pbi.effective_end_date
LEFT JOIN TREASURY.TR_MST_COUNTERPARTIES@FIFAPPS.US.ORACLE.COM TMC
ON  TMC.CP_TYPE = 'BANK' AND pbi.bank_name = tmc.cp_code
LEFT JOIN PEA_ACCOUNT_INFORMATIONS pai
ON pai.person_id   = ppi.person_id
AND pai.company_id = ppi.company_id
AND TRUNC(TO_DATE($P{PROCESS_DATE} ,'DD-MON-YYYY')) BETWEEN pai.effective_start_date AND pai.effective_end_date
LEFT JOIN EWS.BRANCH_HIERARCHY_V BH
ON BH.ORGANIZATION_ID = PPA.ORGANIZATION_ID
LEFT JOIN
  (SELECT pe1.person_id,
    pe1.company_id,
    pe1.education_level,
    pe1.institution,
    pe1.faculty,
    pe1.major
  FROM pea_educations pe1
  WHERE pe1.end_date =
    (SELECT MAX(end_date)
    FROM pea_educations pe2
    WHERE pe2.person_id = pe1.person_id
    AND pe2.company_id  = pe1.company_id
    )
  ) edu
ON edu.person_id   = ppi.person_id
AND edu.company_id = ppi.company_id
LEFT JOIN
  (SELECT pa.person_id,
    pa.company_id,
    pa.address_line,
    fmp.provinsi,
    fmc.city,
    pa.zip_code
  FROM pea_addresses pa,
    fs_mst_provinsi fmp,
    fs_mst_cities fmc
  WHERE pa.province_code = fmp.prov_code
  AND pa.city_code       = fmc.city_code
  AND pa.is_ktp          = 1
  AND TRUNC(TO_DATE($P{PROCESS_DATE} ,'DD-MON-YYYY')) between pa.start_date and pa.end_date
  ) address
ON address.person_id   = ppi.person_id
AND address.company_id = ppi.company_id
LEFT JOIN ter_requests tr
ON tr.person_id        = ppi.person_id
AND tr.company_id      = ppi.company_id
AND tr.approval_status = 'APPROVED'
LEFT JOIN ter_types tt
ON tt.type_id = tr.termination_type_id
WHERE TRUNC(TO_DATE($P{PROCESS_DATE} ,'DD-MON-YYYY')) BETWEEN PPI.EFFECTIVE_START_DATE AND PPI.EFFECTIVE_END_DATE
AND TRUNC(TO_DATE($P{PROCESS_DATE} ,'DD-MON-YYYY')) BETWEEN PPA.EFFECTIVE_START_DATE AND PPA.EFFECTIVE_END_DATE
AND TRUNC(TO_DATE($P{PROCESS_DATE} ,'DD-MON-YYYY')) BETWEEN bc.effective_start_date AND bc.effective_end_date
AND TRUNC(TO_DATE($P{PROCESS_DATE} ,'DD-MON-YYYY')) BETWEEN ppt.effective_start_date AND ppt.effective_end_date
AND (bh.branch_id = $P{BRANCH_ID} OR $P{BRANCH_ID} is null)
AND (ppt.employment_category = $P{EMPLOYMENT_CATEGORY} OR $P{EMPLOYMENT_CATEGORY} is null)
AND (wg.grade = $P{GRADE} OR $P{GRADE} is null)
AND (wg.sub_grade = $P{SUB_GRADE} OR $P{SUB_GRADE} is null)
AND (ppi.hire_date >=  TRUNC(TO_DATE($P{HIRE_DATE_FROM})) OR $P{HIRE_DATE_FROM} is null)
AND (ppi.hire_date <= TRUNC(TO_DATE($P{HIRE_DATE_TO})) OR $P{HIRE_DATE_TO} is null)
AND (ppa.job_id = $P{JOB_ID} OR $P{JOB_ID} is null)
AND (ppi.marital_status = $P{MARITAL_STATUS} OR $P{MARITAL_STATUS} is null)
AND (ppa.organization_id = $P{ORGANIZATION_ID} OR $P{ORGANIZATION_ID} is null)
AND (ppi.company_id = $P{COMPANY_ID} OR $P{COMPANY_ID} is null)
AND $X{IN, ppa.organization_id, AUTHORIZED_ORGANIZATIONS}
ORDER BY BH.BRANCH_NAME ASC, PPI.FULL_NAME asc]]>
	</queryString>
